{
  "manifest_version": 3,
  "name": "Start enhanced openfing",
  "description": "Starts enhanced openfing on https://open.fing.edu.uy/* domains",
  "version": "0.1",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://open.fing.edu.uy/*"
      ]
    }
  ]
}